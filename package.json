{
    "name": "identity-service",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "8.1.3",
        "npm": "5.6.0"
    },
    "scripts": {
        "start": "run-s build watch",
        "build": "run-s clean build-tsc copy-hbs",
        "build-prod": "run-s clean build-tsc-prod copy-hbs",
        "build-tsc": "tsc -p server/tsconfig.dev.json",
        "build-tsc-prod": "tsc -p server/tsconfig.prod.json",
        "copy-hbs": "cpx \"server/**/*.hbs\" dist/server",
        "watch": "run-p \"watch-*\"",
        "watch-tsc": "tsc -p server/tsconfig.dev.json --watch",
        "watch-hbs": "cpx \"server/**/*.hbs\" dist/server --watch --no-initial --verbose",
        "watch-dist": "nodemon --config nodemon.json",
        "lint": "tslint './server/**/*.ts' -c tslint.json --format stylish --force",
        "test": "mocha 'dist/**/*.test.js' --exit",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/consolidate": "0.0.33",
        "@types/js-url": "^1.8.28",
        "@types/qs": "^6.5.1",
        "@types/randomstring": "^1.1.6",
        "@types/underscore": "^1.8.6",
        "@types/underscore.string": "0.0.32",
        "base64url": "^2.0.0",
        "body-parser": "^1.18.2",
        "consolidate": "^0.15.0",
        "express": "~4.15.5",
        "express-handlebars": "^3.0.0",
        "jsrsasign": "^8.0.4",
        "lodash": "^4.17.4",
        "mongodb": "^2.2.33",
        "nosql": "^6.1.0",
        "qs": "^6.5.1",
        "querystring": "^0.2.0",
        "randomstring": "^1.1.5",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@types/chai": "^4.0.4",
        "@types/express": "^4.0.39",
        "@types/lodash": "^4.14.85",
        "@types/mocha": "^2.2.44",
        "@types/mongodb": "^2.2.15",
        "@types/node": "^8.0.51",
        "@types/sinon": "^4.0.0",
        "@types/supertest": "^2.0.4",
        "chai": "^4.1.2",
        "cpx": "^1.5.0",
        "mocha": "^4.0.1",
        "nodemon": "^1.12.1",
        "npm-run-all": "^4.1.2",
        "onchange": "^3.3.0",
        "rimraf": "^2.6.2",
        "sinon": "^4.1.2",
        "supertest": "^3.0.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1"
    }
}
